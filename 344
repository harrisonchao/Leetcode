char* reverseString(char* s) {
    int leng=strlen(s),i=0;
    printf("%d\n",leng);
    char* answer=NULL;
    answer=(char*)malloc((leng+1)*sizeof(char));
    
    for(i=0;i<leng;i++)
    {
        printf("%c\n",s[leng-i-1]);
        answer[i]=s[leng-i-1];
    }
    answer[leng]='\0';
    printf("%s\n",answer);
    return answer;
}
